<div class="posts">
    {{#each posts}}
    <article class="card">
        <div class="c-header">
            <h2><a href="/post/{{id}}">{{title}}</a></h2>
        </div>
        <div class="c-body">
            {{body}}
        </div>
            <div>-{{User.username}} {{format_date createdAt}}</div>
    </article>
    {{/each}}
</div>